/test:
  get:
    body:
      application/json:
        schema: |
          {
            "$schema": "http://json-schema.org/draft-03/schema#",
            "id": "http://gurumeditator.io/schema/my-object.json",
            "type": "object",
            "properties": {
              "data": {
                "type": "array",
                "items": {
                  "id": "http://gurumeditator.io/schema/my-other-object.json",
                  "type": "object",
                  "properties": {
                    "name": {
                      "type": "string",
                      "required": true
                    },
                    "typeName": {
                      "type": "string",
                      "required": true
                    },
                    "placeList": {
                      "type": "string",
                      "required": true
                    }
                  }
                }
              }
            }
          }

